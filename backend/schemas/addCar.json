{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "id": "http://example.com/addCar.schema.json",
    "type": "object",
    "properties": {
      "make": {
        "type": "string",
        "minLength": 5,
        "maxLength": 26
      },
      "model": {
        "type": "string",
        "minLength": 2,
        "maxLength": 26
      },
      "model_year": {
        "type": "string",
        "minLength": 4,
        "maxLength": 4
      }
    },
    "required": [
      "make",
      "model",
      "model_year"
    ]
  }